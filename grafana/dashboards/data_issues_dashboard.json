          "rawQuery": true,
          "rawSql": "WITH file_cnt AS (\n    SELECT DATE(created_at) AS created_date,\n           COUNT(*) AS total_files,\n           SUM(CASE WHEN total_rows IS NULL THEN 1 ELSE 0 END) AS total_bad_csv\n    FROM data_issues\n    WHERE created_at >= $__timeFrom() \n    GROUP BY created_date\n)\nSELECT created_date,\n       f.total_files,\n       f.total_bad_csv,\n       ROUND((f.total_bad_csv * 100.0) / f.total_files, 2) AS bad_csv_percentage\nFROM file_cnt f\n",
